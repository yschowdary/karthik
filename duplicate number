#include<stdio.h>
#include<conio.h>
int main()
{
	int i,arr[20],j,no;
	printf("enter the siz of array");
	scanf("%d",&no);
	printf("enter any %d elements in array:",no);
	for(i=0;i<no;i++)
	{
		scanf("%d",&arr[i]);
	}
	printf("duplicate elements are:");
	for(i=0;i<no;i++)
	{
	for(j=i+1;j<no;j++)
	{
	if(arr[i]==arr[j])
	{
	printf("%d\n",arr[i]);	
	}
	}
    }
    return 0;
}
